m '../../images/wave.svg';\r\n\r\nclass SignIn extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    console.log(this.props)\r\n    this.state = {\r\n      email: '',\r\n      password: '',\r\n      errors: {\r\n        email: '',\r\n        password: ''\r\n      }\r\n    }\r\n\r\n    this.handleSubmit = this.handleSubmit.bind(this);\r\n    this.handleChange = this.handleChange.bind(this);\r\n  }\r\n\r\n  handleChange(event) {\r\n    this.setState({\r\n      [event.target.name]: event.target.value\r\n    });\r\n  }\r\n\r\n  handleSubmit(event) {\r\n    event.preventDefault();\r\n    this.setState({\r\n      errors: {\r\n        email: '',\r\n        password: ''\r\n      }\r\n    });\r\n\r\n    const {email, password} = this.state;\r\n    if(!new RegExp(/^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$/g).test(email)) {\r\n      this.setState({\r\n        errors: {\r\n          email: 'Debes ingresar un email válido',\r\n          password: ''\r\n        }\r\n      });\r\n      return;\r\n    }\r\n\r\n    if(password.length < 7) {\r\n      this.setState({\r\n        errors: {\r\n          email: '',\r\n          password: 'Debes ingresar mínimo 7 caracteres'\r\n        }\r\n      });\r\n      return;\r\n    }\r\n\r\n    if (!new RegExp(\"^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.{7,})\").test(password)) {\r\n      this.setState({\r\n        errors: {\r\n          email: '',\r\n          password: 'La contraseña debe contener al menos 1 mayuscula, 1 minuscula y 1 número'\r\n        }\r\n      });\r\n      return;\r\n    }\r\n\r\n    this.props.login(email, password);\r\n  }\r\n\r\n  render() {\r\n    return (\r\n      <div className=\"w-screen h-screen flex flex-col justify-center items-center bg-gray-200\">\r\n        <img className=\"fixed bottom-0 left-0 right-0\" src={WaveImage} alt=\"Bottom Wave\" />\r\n        <div className=\"shadow border rounded p-4 bg-white z-50\">\r\n          <form noValidate={true} onSubmit={this.handleSubmit} className=\"flex flex-col justify-center items-center z-50\">\r\n            <img src={AvatarImage} alt=\"Avatar\" className=\"w-32\" />\r\n            <h2 className=\"my-8 font-display font-bold text-3xl text-gray-700 text-center\">\r\n              Ingreso\r\n            </h2>\r\n            <div className=\"relative\">\r\n              <FontAwesomeIcon className=\"absolute text-gray-600 text-xl\" icon={['fas', 'user']} />\r\n              <input\r\n                type=\"text\"\r\n                name=\"email\"\r\n                placeholder=\"Correo electrónico\"\r\n                value={this.state.email}\r\n                onChange={this.handleChange}\r\n                className=\"pl-8 border-b-2 font-display text-lg focus:outline-none focus:border-gray-700 transition-all duration-500\"\r\n                required />\r\n            </div>\r\n            {\r\n              this.state.errors.email &&\r\n              <div>\r\n                <small className=\"inline-block text-sm text-red-500\">{this.state.errors.email}</small>\r\n              </div>\r\n            }\r\n            <div className=\"relative mt-8\">\r\n              <FontAwesomeIcon className=\"absolute text-gray-600 text-xl\" icon={['fas', 'lock']} />\r\n              <input\r\n                type=\"password\"\r\n                name=\"password\"\r\n                placeholder=\"Contraseña\"\r\n                value={this.state.password}\r\n                onChange={this.handleChange}\r\n                className=\"pl-8 border-b-2 font-display text-lg focus:outline-none focus:border-gray-700 transition-all duration-500\"\r\n                required />\r\n            </div>\r\n            {\r\n              this.state.errors.password &&\r\n              <div>\r\n                <small className=\"inline-block text-sm text-red-500 break-words\">{this.state.errors.password}</small>\r\n              </div>\r\n            }\r\n            <button className=\"py-3 px-20 bg-blue-500 rounded-full text-white font-bold uppercase text-lg mt-4 transform hover:translate-y-1 transition-all duration-500\" type=\"submit\">Iniciar Sesión</button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nconst mapStateToProps = state => {\r\n  return {\r\n    alert: state.alert,\r\n    auth: state.auth\r\n  }\r\n}\r\n\r\nconst mapDispatchToProps = dispatch => {\r\n  return {\r\n    showErrorAlert: (message) => dispatch(alertActions.error(message)),\r\n    login: (email, password) => dispatch(userActions.login(email, password))\r\n  }\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(SignIn);",["145","146"],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\routes\\Route.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\theme\\components\\navbar.component.js",["147"],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\theme\\components\\sidebar.component.js",["148"],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\theme\\dashboard.theme.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\index.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\redux\\rootReducer.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\redux\\types\\alert.type.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\containers\\main\\index.js",["149","150"],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\routes\\index.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\helpers\\index.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\redux\\reducers\\alert.reducer.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\redux\\reducers\\auth.reducer.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\redux\\actions\\user.action.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\helpers\\auth-header.helper.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\helpers\\history.helper.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\redux\\types\\index.js",[],"F:\\Projects\\Personal\\LAE\\test-reactjs\\src\\redux\\types\\user.type.